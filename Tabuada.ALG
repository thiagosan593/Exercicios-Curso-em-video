algoritmo "Cont1a10-cont "
var
   cont , n, T: inteiro
inicio
   cont <- 0
   // n  <- 7
   ESCREVAL("Deseja a tabuada de qual numero? ")
   leia(n)
   Repita
      T <-  N* CONT
      escrevaL ( N , " X " , CONT , " = ", T)
      //escreva (cont)
      cont <-   cont+1
   ate (cont >10)




fimalgoritmo