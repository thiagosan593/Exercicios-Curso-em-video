algoritmo "conte ate 10 "
var
   contador , numero , salto: inteiro

inicio
   escreval ("Até que numero deseja contar")
   leia (numero)
   escreval (" qual sera o valor do salto")
   leia (salto)
   contador <- 0
   enquanto (contador <= numero) faca
      escreval (contador )
      contador <- contador + salto
   fimenquanto
   escreval("terminei de contar")

fimalgoritmo