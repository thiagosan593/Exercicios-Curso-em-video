algoritmo "quantos entre 0 e 10"
var
   c,v ,tot010 :inteiro
inicio
   tot010 <- 0
   para c<-1 ate 6 faca
      escreva ("digite um valor: ")
      leia (V)
      se(V>=0) e (v<=10) entao
         tot010<- tot010+1
      fimse
   fimpara
   escreval( " Ao  todo foram ", tot010 , "Entre 0 e 10 ")

fimalgoritmo