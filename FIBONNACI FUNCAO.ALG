algoritmo "Fibonnaci funcao"
var
  i, t1 ,t2 ,t3: inteiro
  funcao proximofibonacci (var n1,n2 :inteiro) : inteiro
var
  n3: inteiro
inicio
    N3<- N1+N2
    N1<- N2
    N2<- N3
    RETORNE N3
fimfuncao
inicio
   T1<-0
   Escreval(T1)
   T2<-1
   escreval (T2)
   para i<-3 ate 10 faca
       t3<- proximofibonacci(t1,t2)
       escreval(t3)
    fimpara
fimalgoritmo